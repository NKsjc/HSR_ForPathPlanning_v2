;network settings
;udp port
udp_port=9002
;tcp port
tcp_port=9001
;server ip hic
server_ip=192.168.3.6
;------------------------------------------------------------------------;
;log setting
;log path 
log_path=./log/
;choose the log level exp:  _Debug  _Info  _Warning  _Error
log_level=_Debug
